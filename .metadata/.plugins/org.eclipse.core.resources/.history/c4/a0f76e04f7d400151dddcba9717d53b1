        //  AND difference = K+1
		
		
		if( x.length != y.length)
			return -1 ;

		Map<Integer,Integer> coordinates = new HashMap<Integer,Integer>() ;

		for (int i = 0; i < x.length; i++) {
			coordinates.put(x[i], y[i]) ;
		}

