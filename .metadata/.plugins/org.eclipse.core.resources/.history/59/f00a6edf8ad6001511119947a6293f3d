package dp;

import java.util.HashSet;
import java.util.Set;

public class AllSubsets {

	public static void main(String[] args) {
		Set<Integer> set = new HashSet<Integer>() ;
		set.add(5) ;
		set.add(6) ;
		set.add(7) ;
		
		Set<Integer> result = subsets(set) ;
		
		for (Integer i : result) {
			System.out.println(i);
		}
	}


	public static Set<Integer> subsets( Set<Integer> set ){

		int n = set.size() ;

		if( n == 0 )
			return null ;

		else if( n == 1){
			return set ;
		}
		else if( n > 1){
			
			int[] arr = new int[set.size()] ;
			set.toArray() ;
			int current = arr[0] ;
			HashSet<Integer> newSet = new HashSet<Integer>();
			
			for (int i = 1; i < arr.length; i++) {
				newSet.add(arr[i]) ;
			}
			subsets(newSet).add(current) ; 
			return newSet ;
		}
		return null;

	}

}
