
public class SummaryRanges {

	int a[] = { 2, 3, 4, 5, 12, 14, 15, 16, 17, 19 } ;

	public static void main(String[] args) {

	}

	public void printRanges(){

		// Indices for high & lows
		int low = 0 ;
		int high = 0 ;
		int j = 0 ;
		StringBuilder sb = new StringBuilder() ;
		
		while( j < a.length){

			if( a[j] - a[j-1] == 1){
				high = j ;
				j++ ;
			}else{
				sb.append(low + "->" + high) ;
				low = j ;
			}
		}

	}
}
