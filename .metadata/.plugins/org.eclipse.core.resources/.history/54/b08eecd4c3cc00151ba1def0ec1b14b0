
public class SpiralMatrix {

	static int[][] box = { {1,2,3} , {4,5,6}, {7,8,9}};

	public static void main(String[] args) {
		printSpirally() ;
	}

	private static void printSpirally() {
		int i, j ;
		for ( i = 0; i < box.length; i++) {
			if( i % 2 == 0){
				for ( j = 0; j < box[i].length; j++) {
					System.out.println( box[ i][j]);
				}
			}else{
				for ( j = box[i].length-1 ; j>= 0; j++) {
					System.out.println( box[ i][j]);
				}
			}
		}
		
	}

}
