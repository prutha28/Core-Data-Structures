package string.operations.using.stacks;

import fixed.size.stacks.Stack;

public class PalindromeCheck {

	private String word = "";
	
	public static void main(String[] args) {
		new PalindromeCheck().checkForPalindrome() ;
	}

	private void checkForPalindrome() {
		Stack palStack = new Stack() ;
		pushWordOnStack(palStack);
	}

	private void pushWordOnStack(Stack palStack) {
		for (int i = 0; i < word.length(); i++) {
			palStack.push(word.charAt(i)) ;
		}
	}

}
